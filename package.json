{
  "name": "phenohunterapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest tests/unit",
    "test:integration": "vitest tests/integration",
    "test:security": "vitest run --grep security",
    "test:performance": "vitest run --grep performance",
    "audit:security": "npm audit --audit-level moderate",
    "security:check": "scripts\\security-check.bat",
    "security:check:unix": "bash scripts/security-check.sh",
    "validate:code": "npm run test && npm run audit:security && npm run build",
    "pre-commit": "npm run security:check && npm run validate:code"
  },
  "dependencies": {
    "@react-pdf/renderer": "^4.3.0",
    "chart.js": "^4.5.0",
    "d3-interpolate": "^3.0.1",
    "d3-scale": "^4.0.2",
    "firebase": "^12.0.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "ml-regression": "^6.3.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.26.1",
    "react-spring": "^10.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "jest-environment-jsdom": "^30.0.5",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "vite": "^5.2.0",
    "vitest": "^3.2.4"
  }
}
